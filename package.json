  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "phaser": "^3.17.0"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "concurrently": "^4.1.2",
    "require-globify": "^1.4.1",
    "watchify": "^3.11.1"
  }
}
