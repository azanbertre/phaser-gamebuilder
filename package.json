{
  "name": "phaser-gamebuilder",
  "version": "0.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "browserify -t require-globify ./index.js --s GameBuilder > ./dist/gamebuilder.js",
    "demo": "browserify -t require-globify ./index.js --s GameBuilder > ./demo/js/gamebuilder.js && cd ./demo && node ./run.js",
